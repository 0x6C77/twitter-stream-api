{
  "name": "redwebcreation/tweets-stream",
  "description": "A PHP interface to the Twitter Streaming API.",
  "keywords": [
    "redwebcreation",
    "phirehose-replacement",
    "phirehose",
    "php",
    "twitter",
    "streaming",
    "api"
  ],
  "type": "library",
  "homepage": "https://github.com/redwebcreation/phirehose",
  "license": "MIT",
  "authors": [
    {
      "name": "FÃ©lix Dorn",
      "email": "github@felixdorn.fr",
      "homepage": "https://redwebcreation.fr/"
    }
  ],
  "require": {
    "php": "^8",
    "spatie/enum": "^3.8",
    "friendsofphp/php-cs-fixer": "^3",
    "phpstan/phpstan": "^0.12.85",
    "guzzlehttp/guzzle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "RWC\\Phirehose\\": "src/"
    }
  },
  "scripts": {
    "lint": "php-cs-fixer fix -v",
    "test:lint": "php-cs-fixer fix -v --dry-run",
    "test:types": "phpstan analyse --ansi --memory-limit=0",
    "test": [
      "@test:lint",
      "@test:types"
    ]
  },
  "require-dev": {
    "symfony/var-dumper": "^5.2"
  }
}
